              "                                                                          'Romance',\n",
              "                                                                          'Sci-Fi'...\n",
              "                                                                         'description')])),\n",
              "                                       ('to_dense',\n",
              "                                        FunctionTransformer(accept_sparse=True,\n",
              "                                                            func=<function <lambda> at 0x7f2be1ab7380>)),\n",
              "                                       ('model',\n",
              "                                        RandomForestRegressor(n_jobs=-1,\n",
              "                                                              random_state=42))]),\n",
              "             n_jobs=-1,\n",
              "             param_grid={'model__max_depth': [None, 20],\n",
              "                         'model__max_features': ['sqrt'],\n",
              "                         'model__min_samples_leaf': [1, 2],\n",
